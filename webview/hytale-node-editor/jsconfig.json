{
	"compilerOptions": {
		"target": "ES2022",
		"module": "ESNext",
		"moduleResolution": "Bundler",
		"allowJs": true,
		"checkJs": false,
		"resolveJsonModule": true,
		"isolatedModules": true,
		"types": [
			"svelte"
		]
	},
	"include": [
		"src/**/*.js",
		"src/**/*.svelte",
		"vite.config.mjs"
	],
	"exclude": [
		"node_modules",
		"../../node_modules",
		"../../dist",
		"../../out",
		"../../media"
	]
}
